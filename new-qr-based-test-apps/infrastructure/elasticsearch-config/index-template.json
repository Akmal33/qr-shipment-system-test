{
  "template": {
    "settings": {
      "number_of_shards": 2,
      "number_of_replicas": 0,
      "refresh_interval": "5s",
      "max_result_window": 50000
    },
    "mappings": {
      "properties": {
        "timestamp": {
          "type": "date",
          "format": "strict_date_optional_time||epoch_millis"
        },
        "eventType": {
          "type": "keyword"
        },
        "eventId": {
          "type": "keyword"
        },
        "data": {
          "type": "object",
          "dynamic": true
        }
      }
    }
  },
  "index_patterns": ["warehouse-*"],
  "priority": 500
}